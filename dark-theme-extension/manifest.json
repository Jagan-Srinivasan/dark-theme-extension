{
  "manifest_version": 3,
  "name": "Dark Theme Browser",
  "version": "1.0",
  "description": "Applies a dark theme to all websites",
  "permissions": [],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "48": "icon48.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["dark.css"],
      "run_at": "document_start"
    }
  ]
}
